---
import Layout from '@/layouts/Layout.astro';
import Landing from '@/containers/Landing.astro';
import About from '@/containers/About.astro';
import Services from '@/containers/Services.astro';
import Examples from '@/containers/Examples.astro';
import Contact from '@/containers/Contact.astro';
import Footer from '@/containers/Footer.astro';
import ScrollingWords from '@/components/ScrollingWords.jsx';
import BGChanger from '@/components/BGChanger.jsx';
import LargeCTA from '@/containers/LargeCTA.astro';
---

<Layout>
  <Landing />
  <About />
  <BGChanger client:load>
    <ScrollingWords client:visible />
    <Examples />
    <LargeCTA />
    <Services />
    <Contact />
    <Footer />
  </BGChanger>
</Layout>
